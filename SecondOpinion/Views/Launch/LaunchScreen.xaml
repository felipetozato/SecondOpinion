<?xml version="1.0" encoding="UTF-8"?>
<ui:ContentPageBase xmlns="http://xamarin.com/schemas/2014/forms" 
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    x:Class="SecondOpinion.Views.Launch.LaunchScreen"
                    xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
                    xmlns:viewModels="clr-namespace:SecondOpinion.ViewModels"
                    xmlns:ui="clr-namespace:SecondOpinion.Views"
                    x:TypeArguments="viewModels:AppInitializerViewModel">
    <ContentPage.Content>
        <ActivityIndicator x:Name="ActivityIndicator"
                           IsRunning="true"
                           IsVisible="true" Color="Teal"
                           VerticalOptions="Center"
                           HorizontalOptions="Center"/>
    </ContentPage.Content>
</ui:ContentPageBase>
